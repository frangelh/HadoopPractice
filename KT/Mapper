#!/home/deamon3/bin/kscript

fun main(args: Array<String>) {
    val lines : List<String> = args[0].split("\n")

   lines.forEach {
        val words: List<String> = it.split("\t")
        val lugar = words[0]
        val mes = words[1]
        val cantidad = words[2]
        println(" $lugar\t$mes\t$cantidad \n")
    }
}